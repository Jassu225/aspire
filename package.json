{
  "name": "aspire",
  "version": "0.0.1",
  "description": "A mock banking admin application using a mock backend ecosystem that runs entirely in the browser. Built with Vue.js 3, Quasar (with Vite + Typescript), Tailwindcss, IndexedDB (as mock DB), firebase hosting.",
  "productName": "aspire",
  "author": "Jaswanth Sai Sattenapalli <jaswanthsaisattenapalli@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "serve": "quasar serve dist/spa",
    "test:unit": "TEST_TYPE=unit vitest src/**/*.vitest.spec.ts",
    "test:unit:ci": "TEST_TYPE=unit vitest run src/**/*.vitest.spec.ts",
    "test:integration": "TEST_TYPE=integration vitest src/**/*.vitest.integration.test.ts",
    "test:integration:ci": "TEST_TYPE=integration vitest run src/**/*.vitest.integration.test.ts"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "firebase-tools": "^14.12.1",
    "nanoid": "^5.1.5",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "tailwindcss": "^4.1.11",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@playwright/test": "^1.55.0",
    "@quasar/app-vite": "^2.1.0",
    "@quasar/cli": "^2.5.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "^20.5.9",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "fake-indexeddb": "^6.1.0",
    "globals": "^15.12.0",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.9.0",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
